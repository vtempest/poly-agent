{
  "name": "grab-url",
  "description": "ðŸ“¥ Generate Request to API from Browser",
  "type": "module",
  "version": "1.0.18",
  "author": "vtempest",
  "license": "rights.institute/prosper",
  "repository": {
    "type": "git",
    "url": "https://github.com/vtempest/GRAB-URL"
  },
  "main": "./dist/grab-api.cjs.js",
  "types": "./dist/grab-api.d.ts",
  "module": "./dist/grab-api.es.js",
  "bin": {
    "grab-url": "./src/grab-url.js"
  },
  "exports": {
    ".": {
      "types": "./dist/grab-api.d.ts",
      "import": "./dist/grab-api.es.js",
      "require": "./dist/grab-api.cjs.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "import": "./dist/icons.es.js",
      "require": "./dist/icons.cjs.js"
    },
    "./icons/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    },
    "./log": {
      "types": "./dist/log.d.ts",
      "import": "./dist/log.es.js",
      "require": "./dist/log.cjs.js"
    }
  },
  "files": [
    "dist",
    "src",
    "src/grab-api.ts",
    "src/grab-url.js",
    "src/icons/cli/spinners.json"
  ],
  "scripts": {
    "down": "bun src/grab-url.js https://releases.ubuntu.com/24.04.2/ubuntu-24.04.2-live-server-amd64.iso",
    "make": "npm run make:icons; npm run make:docs; npm run make:main",
    "make:docs": "cd docs && npm run build",
    "make:icons": "npx export-svg-typescript@latest -i ./src/icons/svg -o ./src/icons/svg/index.ts",
    "make:main": "vite build",
    "ship": "bun x standard-version --release-as patch; npm publish",
    "check": "vitest --ui",
    "docs": "cd docs && npm run start"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "export-svg-typescript": "^0.1.15",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "api",
    "axios",
    "request",
    "browser",
    "tanstack",
    "query",
    "post",
    "mock-api",
    "fetch",
    "http"
  ],
  "dependencies": {
    "@types/node": "^25.0.3",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "cli-spinners": "^3.2.1",
    "cli-table3": "^0.6.5",
    "ora": "^9.0.0"
  }
}
